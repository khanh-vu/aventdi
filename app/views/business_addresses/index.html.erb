<p id="notice"><%= notice %></p>

<h1>Business Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Business listing</th>
      <th>Address</th>
      <th>Lat</th>
      <th>Lng</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_addresses.each do |business_address| %>
      <tr>
        <td><%= business_address.name %></td>
        <td><%= business_address.business_listing_id %></td>
        <td><%= business_address.address %></td>
        <td><%= business_address.lat %></td>
        <td><%= business_address.lng %></td>
        <td><%= link_to 'Show', business_address %></td>
        <td><%= link_to 'Edit', edit_business_address_path(business_address) %></td>
        <td><%= link_to 'Destroy', business_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business Address', new_business_address_path %>
